{
  "@context": {
    "hydra": "http://www.w3.org/ns/hydra/core#",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "owl": "http://www.w3.org/2002/07/owl#",
    "vs": "http://www.w3.org/2003/06/sw-vocab-status/ns#",
    "dc": "http://purl.org/dc/terms/",
    "cc": "http://creativecommons.org/ns#",
    "schema": "http://schema.org/",
    "qudt": "http://qudt.org/2.1/vocab/",
    "sosa": "http://www.w3.org/ns/sosa/",
    "ssn": "http://www.w3.org/ns/ssn/",
    "cf": "http://cfconventions.org/Data/cf-standard-names/47/build/cf-standard-name-table.html#",

    "defines": {
      "@reverse": "rdfs:isDefinedBy"
    },

    "cc:license": {
      "@type": "@id"
    },
    "cc:attributionURL": {
      "@type": "@id"
    },

    "comment": "rdfs:comment",
    "label": "rdfs:label",
    "preferredPrefix": "http://purl.org/vocab/vann/preferredNamespacePrefix",
    "domain": {
      "@id": "rdfs:domain",
      "@type": "@vocab"
    },
    "range": {
      "@id": "rdfs:range",
      "@type": "@vocab"
    },
    "subClassOf": {
      "@id": "rdfs:subClassOf",
      "@type": "@vocab"
    },
    "subPropertyOf": {
      "@id": "rdfs:subPropertyOf",
      "@type": "@vocab"
    },
    "seeAlso": {
      "@id": "rdfs:seeAlso",
      "@type": "@id"
    },
    "domainIncludes": {
      "@id": "schema:domainIncludes",
      "@type": "@id"
    },
    "rangeIncludes": {
      "@id": "schema:rangeIncludes",
      "@type": "@id"
    },
    "term_status": "vs:term_status",
    "sameAs": "owl:sameAs"
  },

  "@id": "https://urbanobservatory.github.io/standards/latest/ns",
  "@type": "owl:Ontology",

  "label": "The Urban Observatory Core Vocabulary",
  "comment": "A vocabulary for linked sensor networks and observations",
  "preferredPrefix": "uo",

  "dc:description": "The Urban Observatory vocabulary provides a consistent means of describing sensors and monitoring networks, and their relationship to infrastructure systems.",
  "dc:rights": "Copyright Â© 2020 Newcastle University, University of Birmingham, University of Sheffield, University of Bristol, Cranfield University, University of Manchester",
  "dc:publisher": "Urban Observatory",
  "cc:license": "http://creativecommons.org/licenses/by/4.0/",
  "cc:attributionName": "Urban Observatory",
  "cc:attributionURL": "https://www.urbanobservatory.ac.uk/",

  "defines": [
    {
      "@id": "uo:Collection",
      "@type": "rdfs:Class",
      "label": "collection",
      "comment": "A collection of resources",
      "term_status": "unstable",
      "subClassOf": "rdfs:Container"
    },
    {
      "@id": "uo:hasMember",
      "@type": "rdf:Property",
      "label": "has member",
      "comment": "A member of a collection",
      "term_status": "unstable",
      "domain": "uo:Collection",
      "subPropertyOf": "rdf:member"
    },
    {
      "@id": "Discipline",
      "sameAs": "qudt:discipline",
      "comment": "The fields of expertise to which an observation is relevent.",
      "domain": "ssn:Observation",
      "term_status": "unstable"
    },
    {
      "@id": "uo:Meteorology",
      "@type": "uo:Discipline",
      "label": "Meteorology",
      "comment": "The scientific study of the atmosphere that focuses on weather processes and forecasting.",
      "sameAs": "qudt:discipline:Meteorology",
      "term_status": "unstable"
    },
    {
      "@id": "recommendedUnit",
      "@type": "rdf:Property",
      "domain": "sosa:ObservableProperty",
      "label": "Observations with this observed property are typically provided in this unit of measure. Other units may still be used but the alternative unit MUST be explicitly defined.",
      "term_status": "unstable"
    },
    {
      "@id": "AirTemperature",
      "@type": "sosa:ObservableProperty",
      "recommendedUnit": "qudt:unit:DEG_C",
      "comment": "Air temperature is the bulk temperature of the air, not the surface (skin) temperature. A dry-bulb temperature.",
      "sameAs": "cf:air_temperature_tr"
    }
  ]
}